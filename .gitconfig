[user]
	name = ####### 
	email = ####### 
[core]
	editor = vim
    pager = more
[color]
    ui = auto
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	st = status
	co = checkout
	br = branch
	up = rebase
	ci = commit
	ll = log --pretty=format:\"%C(yellow)%h%Cred%d%Creset %s %Cgreen(%cr)%Cblue [%cn]\" --decorate --numstat
